{
  "name": "@sasaplus1/keystroke",
  "description": "manage keystroke",
  "version": "2.0.0",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "devDependencies": {
    "@ast-grep/cli": "0.32.3",
    "@vitest/browser": "2.1.8",
    "esbuild": "0.24.2",
    "typescript": "5.7.2",
    "vitest": "2.1.8"
  },
  "engines": {
    "bun": "1",
    "deno": "2",
    "node": "22",
    "npm": "11"
  },
  "exports": "./dist/mod.js",
  "files": [
    "./LICENSE",
    "./README.md",
    "./dist"
  ],
  "license": "MIT",
  "main": "./dist/mod.js",
  "readmeFilename": "./README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sasaplus1/keystroke.js.git"
  },
  "scripts": {
    "build": "esbuild --outdir=./dist ./main.ts ./mod.ts && sg scan -U -r ./replace-extension.yml ./dist/*.js && tsc -p ./tsconfig.type.jsonc",
    "check": "deno check ./main.ts ./mod.ts",
    "clean": "rm -rf ./dist",
    "postinstall": "deno install --lock --allow-scripts=npm:@ast-grep/cli@0.32.3 || true",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run"
  },
  "sideEffects": false,
  "type": "module"
}
